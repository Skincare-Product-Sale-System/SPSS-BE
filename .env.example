# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to Git

# Database
SPSS_CONNECTION_STRING=Server=localhost;Database=spss;User ID=sa;Password=YOUR_PASSWORD;TrustServerCertificate=True

# JWT Configuration
JWT_SECRET_KEY=your-secret-key-minimum-64-characters-long
JWT_ISSUER=https://spssapi-hxfzbchrcafgd2hg.southeastasia-01.azurewebsites.net
JWT_AUDIENCE=https://skincede-spss.vercel.app

# Face++ API
FACEPP_API_KEY=your-facepp-api-key
FACEPP_API_SECRET=your-facepp-api-secret

# Banking Information
BANK_NAME=YOUR_BANK_NAME
BANK_ID=YOUR_BANK_ID
BANK_ACCOUNT_NUMBER=YOUR_ACCOUNT_NUMBER
